{
  "name": "{{name}}",
  "version": "0.0.0-semantic-release",
  "description": "",
  "module": "{{module}}",
  "main": "{{main}}",
  "keywords": [],
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "docs": "jsdoc2md -l off -t README.hbs -f {{module}} > README.md",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{{github.user}}/{{github.repo}}.git"
  },
  "bugs": {
    "url": "https://github.com/{{github.user}}/{{github.repo}}/issues"
  },
  "homepage": "https://github.com/{{github.user}}/{{github.repo}}#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.0",
    "semantic-release": "^6.3.1"
  },
  "contributors": [{
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
